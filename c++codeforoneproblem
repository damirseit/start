  #include <cmath>
#include <math.h>
#include <iostream>
using namespace std;

int main()
{
int x1,y1,r1,x2,y2,r2;

cin >> x1 >> y1 >> r1;
cin>> x2 >> y2 >> r2;
if (-5000<=x1,x2,y1,y2<=5000 && 1<=r1,r2<=1000)
{


if (sqrt((x1-x2)^2+(y1-y2)^2)>=r1+r2)
{
    cout<<"YES";
}
else
{
    cout<<"NO";
}
}

}
